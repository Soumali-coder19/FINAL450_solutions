 def sort012(self,arr,n):
        # code here
        zero,one,two=0,0,0
        for i in arr:
            if i==0:
                zero+=1
            elif i==1:
                one+=1
            elif i==2:
                two+=1
        arr.clear()
        arr+=[0]*zero
        arr+=[1]*one
        arr+=[2]*two
        return arr
